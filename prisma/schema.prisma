generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  relationMode      = "prisma"
}

model Exercise {
  id     Int    @id @default(autoincrement())
  name   String
  sets   Int
  reps   Int
  weight Int
  unit   String
  status String
  type String
}
